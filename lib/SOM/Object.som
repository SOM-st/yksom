Object = nil (
    class = primitive
    asString  = ( ^'instance of ' concatenate: (self class asString) )

    =  other = ( ^self == other )
    <> argument = ( ^(self = argument) not )
    == other = primitive
    ~= other = (^ (self == other) not )

    print = ( self asString print )
    println = (
        self print.
        system printNewline
    )
)
