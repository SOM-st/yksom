Double = (
    + argument = primitive
    - argument = primitive
    * argument = primitive
    =  argument = primitive
    <  argument = primitive
    >  argument = ( ^(self >= argument) and: [ self <> argument ] )
    >= argument = ( ^(self < argument) not )
    <= argument = ( ^(self < argument) or: [ self = argument ] )
    negative = ( ^self < 0.0 )
    sqrt     = primitive
    asString = primitive
)
